    rdfs:domain :BuscarProducto ;
    rdfs:range :Restricciones .

:RetornaUn a owl:ObjectProperty ;
    rdfs:domain :PeticionRetorno ;
    rdfs:range :Producto .

:Stock1 a :Stock,
        owl:NamedIndividual ;
    :Tiene :Producto2 ;
    :UnidadesEnStok "999999"^^xsd:int .

:Tarjeta a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Factura :PeticionCompra :PeticionTransferencia ) ] ;
    rdfs:range xsd:int .

