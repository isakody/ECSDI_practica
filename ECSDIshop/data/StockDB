:Ofrece a owl:ObjectProperty ;
    rdfs:domain :Transportista ;
    rdfs:range :OfertaDeTransporte .

:Pagado a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :PeticionCompra ;
    rdfs:range xsd:boolean .

:Para a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :OfertaDeTransporte :PeticionOfertaTransporte ) ] ;
    rdfs:range :LoteProductos .

:PendienteDeSerEnviado a owl:ObjectProperty ;
    rdfs:domain :EnviarUnLote ;
    rdfs:range :LoteProductos .

:PerteneceA a owl:ObjectProperty ;
    rdfs:domain :Valoracion ;
    rdfs:range :Producto ;
    rdfs:subPropertyOf owl:topObjectProperty .

:Peso a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :LoteProductos :Producto ) ] ;
    rdfs:range xsd:float .

:PeticionTransporte a owl:Class ;
    rdfs:subClassOf :Accion .

:Precio a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :Producto ;
    rdfs:range xsd:float .

:PrecioMaximo a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :RestriccionDePrecio ;
    rdfs:range xsd:float .

:PrecioMinimo a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :RestriccionDePrecio ;
    rdfs:range xsd:float .

:PrecioTotal a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Factura :PeticionCompra :PeticionTransferencia ) ] ;
    rdfs:range xsd:float .

:Prioridad a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :LoteProductos,
        :PeticionCompra,
        :PeticionEnvioACentroLogístico,
        :ProductoExterno,
        :RespuestaEnvioDesdeCentroLogistico ;
    rdfs:range xsd:int .

:Productos a owl:ObjectProperty ;
    rdfs:domain :PeticionRegistroProductos ;
    rdfs:range :ProductoExterno .

:ProximoA a owl:ObjectProperty ;
    rdfs:domain :CentroLogistico ;
    rdfs:range :Direccion .

:Puntuacion a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :Valoracion ;
    rdfs:range xsd:int .

