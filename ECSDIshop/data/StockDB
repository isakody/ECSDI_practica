:Producto2 a :Producto,
        owl:NamedIndividual ;
    :Descripcion "Esto es un mouse"^^xsd:string ;
    :Id "2"^^xsd:int ;
    :Nombre "Mouse"^^xsd:string ;
    :Peso "1.0"^^xsd:float ;
    :Precio "32.0"^^xsd:float .

