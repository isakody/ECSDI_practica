@prefix : <http://www.owl-ontologies.com/ECSDIstore#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.owl-ontologies.com/ECSDIstore> a owl:Ontology .

:CodigoPostal a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :Direccion ;
    rdfs:range xsd:int .

:CompuestoPor a owl:ObjectProperty ;
    rdfs:domain :LoteProductos ;
    rdfs:range :Producto .

:ConstaDe a owl:ObjectProperty ;
    rdfs:domain :CentroLogistico ;
    rdfs:range :Stock .

:Contiene a owl:ObjectProperty ;
    rdfs:domain :Compra ;
    rdfs:range :Producto .

:De a owl:ObjectProperty ;
    rdfs:domain :PeticionCompra ;
    rdfs:range :Compra .

