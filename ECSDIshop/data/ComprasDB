        owl:FunctionalProperty ;
    rdfs:domain :Stock ;
    rdfs:range xsd:int .

:VendidoPor a owl:ObjectProperty ;
    rdfs:domain :ProductoExterno ;
    rdfs:range :Vendedor .

ns1:PeticionCompra31 a ns1:PeticionCompra ;
    ns1:De ns1:Compra33 ;
    ns1:Prioridad "1"^^xsd:int ;
    ns1:Tarjeta "101010"^^xsd:int .

ns1:PeticionCompra39 a ns1:PeticionCompra ;
    ns1:De ns1:Compra41 ;
    ns1:Prioridad "5"^^xsd:int ;
    ns1:Tarjeta "101017"^^xsd:int .

:BuscarProducto a owl:Class ;
    rdfs:subClassOf :Accion .

:EnviarUnLote a owl:Class ;
    rdfs:subClassOf :Accion .

:Envio a owl:Class .

