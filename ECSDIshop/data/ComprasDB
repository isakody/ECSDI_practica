    rdfs:subClassOf :Accion .

:PeticionRetorno a owl:Class ;
    rdfs:subClassOf :Accion .

:PeticionTransferencia a owl:Class ;
    rdfs:subClassOf :Accion .

:Producto1 a ns1:Producto ;
    ns1:Descripcion "Esto son Patatas"^^xsd:string ;
    ns1:Nombre "Patatas"^^xsd:string ;
    ns1:Peso "23.0"^^xsd:float ;
    ns1:Precio "308.0"^^xsd:float .

:Producto3 a ns1:Producto ;
    ns1:Descripcion "Esto es un teclado"^^xsd:string ;
    ns1:Nombre "Teclado"^^xsd:string ;
    ns1:Peso "2.0"^^xsd:float ;
    ns1:Precio "55.0"^^xsd:float .

:RespuestaDeBusqueda a owl:Class ;
    rdfs:subClassOf :Respuesta .

:Transportista a owl:Class .

:Vendedor a owl:Class .

ns1:Compra33 a ns1:Compra ;
    ns1:Contiene :Producto1 ;
    ns1:Destino ns1:Direccion32 .

ns1:Compra41 a ns1:Compra ;
    ns1:Contiene :Producto3 ;
    ns1:Destino ns1:Direccion40 .

ns1:Direccion32 a ns1:Direccion ;
    ns1:CodigoPostal "1010"^^xsd:int ;
    ns1:Direccion "dolors masferrer"^^xsd:string .

ns1:Direccion40 a ns1:Direccion ;
    ns1:CodigoPostal "2585"^^xsd:int ;
    ns1:Direccion "Calle nicaragua"^^xsd:string .

:CentroLogistico a owl:Class .

