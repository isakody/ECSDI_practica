@prefix : <http://www.owl-ontologies.com/ECSDIstore#> .
@prefix ns1: <http://www.owl-ontologies.com/ECSDIstore.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.owl-ontologies.com/ECSDIstore> a owl:Ontology .

:CodigoPostal a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :Direccion ;
    rdfs:range xsd:int .

:CompuestoPor a owl:ObjectProperty ;
    rdfs:domain :LoteProductos ;
    rdfs:range :Producto .

:ConstaDe a owl:ObjectProperty ;
    rdfs:domain :CentroLogistico ;
    rdfs:range :Stock .

:Contiene a owl:ObjectProperty ;
    rdfs:domain :Compra ;
    rdfs:range :Producto .

:De a owl:ObjectProperty ;
    rdfs:domain :PeticionCompra ;
    rdfs:range :Compra .

:Descripcion a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :Producto ;
    rdfs:range xsd:string .

:DestinadaA a owl:ObjectProperty ;
    rdfs:domain :PeticionOfertaTransporte ;
    rdfs:range :Direccion .

:Destino a owl:ObjectProperty ;
    rdfs:domain :Compra ;
    rdfs:range :Direccion .

:EnvioDe a owl:ObjectProperty ;
    rdfs:domain :PeticionEnvioACentroLogístico ;
    rdfs:range :Producto .

:FechaEntrega a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :OfertaDeTransporte ;
    rdfs:range xsd:dateTime .

:FechaEntregaMaxima a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :IntervaloDeEntrega ;
    rdfs:range xsd:dateTime .

:FechaEntregaMinima a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :IntervaloDeEntrega ;
    rdfs:range xsd:dateTime .

:Id a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:range xsd:int .

:Nombre a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:range xsd:string .

:Ofrece a owl:ObjectProperty ;
    rdfs:domain :Transportista ;
    rdfs:range :OfertaDeTransporte .

:Para a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :OfertaDeTransporte :PeticionOfertaTransporte ) ] ;
    rdfs:range :LoteProductos .

:ParaLa a owl:ObjectProperty ;
    rdfs:domain :PeticionTransferencia ;
    rdfs:range :Compra .

:PendienteDeSerEnviado a owl:ObjectProperty ;
    rdfs:domain :EnviarUnLote ;
    rdfs:range :LoteProductos .

:PerteneceA a owl:ObjectProperty ;
    rdfs:domain :Valoracion ;
    rdfs:range :Producto ;
    rdfs:subPropertyOf owl:topObjectProperty .

:Peso a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :Producto ;
    rdfs:range xsd:float .

:PeticionTransporte a owl:Class ;
    rdfs:subClassOf :Accion .

:Precio a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :Producto ;
    rdfs:range xsd:float .

:PrecioMaximo a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :RestriccionDePrecio ;
    rdfs:range xsd:float .

:PrecioMinimo a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :RestriccionDePrecio ;
    rdfs:range xsd:float .

:Prioridad a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :PeticionCompra,
        :PeticionEnvioACentroLogístico ;
    rdfs:range xsd:int .

:Productos a owl:ObjectProperty ;
    rdfs:domain :PeticionRegistroProductos ;
    rdfs:range :ProductoExterno .

